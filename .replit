modules = ["python-3.11"]
[agent]
integrations = ["python_gemini:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["cargo", "glibcLocales", "libiconv", "rustc"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Streamlit Medical App"

[[workflows.workflow]]
name = "Streamlit Medical App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 34355
externalPort = 3000
